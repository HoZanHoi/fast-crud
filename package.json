{
  "name": "fast-crud-root",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "docgen:crud": "vue-docgen -c docgen/crud.config.js",
    "docgen:extends": "vue-docgen -c docgen/extends.config.js",
    "docs:all": "npm run docgen:crud && npm run docgen:extends && npm run docs:build",
    "publish:pre": "lerna run build --scope */fast-crud --scope */extends-*",
    "publish": "lerna publish --scope */fast-crud --scope */extends-*"
  },
  "author": "Greper",
  "license": "MIT",
  "devDependencies": {
    "lerna": "^4.0.0",
    "vitepress": "^0.13.0",
    "vue-docgen-cli": "^4.37.0"
  }
}
